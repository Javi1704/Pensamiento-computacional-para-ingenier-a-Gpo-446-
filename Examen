#5
P_h=int(input())
a_h=int(input())

P_p=int(input())
a_p=int(input())

area_hexagono=((P_h*a_h)/2)
area_pentagono=((P_p*a_p)/2)

print(area_hexagono)
print(area_pentagono)

#12
B=int(input())
H=int(input())

area_triangulo =((B*H)/2)
print(area_triangulo)