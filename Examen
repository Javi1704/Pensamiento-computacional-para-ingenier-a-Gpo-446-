#1
Current=int(input())
Resistance=int(input())

Voltage=(Current * Resistance)
print(Voltage)

#2
Precio_2022=int(input())
Precio_2023=8000

diferencia=(Precio_2023-Precio_2022)

incremento= ((diferencia/Precio_2022)*100)

print(diferencia)
print(incremento)

#3 
